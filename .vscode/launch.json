  "version": "0.2.0",
  "configurations": [
    {
      "name": "DynamoDB",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd",
      "env": {
        "AWS_PROFILE": "dev"
      },
      "args": ["test/features/aws/dynamodb.feature"],
      "console": "integratedTerminal"
    },
    {
      "name": "RDS",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd",
      "env": {
        "AWS_PROFILE": "dev",
        "AWS_ACCESS_KEY_ID": "test",
        "AWS_SECRET_ACCESS_KEY": "test",
        "AWS_DEFAULT_REGION": "us-east-1",
        "AWS_ENDPOINT_URL": "http://localhost:4566"
      },
      "args": ["../features/aws/rds/rds_db_instance.feature"],
      "console": "integratedTerminal"
    },
    {
      "name": "Hello World",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd",
      "env": {
        "AWS_PROFILE": "dev"
      },
      "args": ["test/features/terraform/hello_world.feature"],
      "console": "integratedTerminal"
    }
  ]
}
